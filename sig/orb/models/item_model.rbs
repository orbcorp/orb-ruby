module Orb
  module Models
    type item_model =
      {
        id: String,
        created_at: Time,
        external_connections: ::Array[Orb::Models::ItemExternalConnectionModel],
        name: String
      }

    class ItemModel < Orb::BaseModel
      attr_accessor id: String

      attr_accessor created_at: Time

      attr_accessor external_connections: ::Array[Orb::Models::ItemExternalConnectionModel]

      attr_accessor name: String

      def initialize:
        (
          id: String,
          created_at: Time,
          external_connections: ::Array[Orb::Models::ItemExternalConnectionModel],
          name: String
        ) -> void
        | (?Orb::Models::item_model | Orb::BaseModel data) -> void

      def to_hash: -> Orb::Models::item_model
    end
  end
end
